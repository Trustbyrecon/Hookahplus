# Dashboard App Local Environment Configuration
# This file contains environment variables specific to the dashboard app

# Dashboard App Configuration
NEXT_PUBLIC_DASHBOARD_APP_NAME=Hookah+ Dashboard
NEXT_PUBLIC_DASHBOARD_VERSION=1.0.0-local
NEXT_PUBLIC_DASHBOARD_ENV=local

# Dashboard Features
NEXT_PUBLIC_DASHBOARD_ENABLE_REFLEX_PANEL=true
NEXT_PUBLIC_DASHBOARD_ENABLE_ONBOARDING=true
NEXT_PUBLIC_DASHBOARD_ENABLE_ANALYTICS=true
NEXT_PUBLIC_DASHBOARD_ENABLE_USER_MANAGEMENT=true

# Dashboard API Configuration
NEXT_PUBLIC_DASHBOARD_API_BASE_URL=http://localhost:3000/api/dashboard
NEXT_PUBLIC_DASHBOARD_API_TIMEOUT=30000

# Dashboard Reflex Scoring
NEXT_PUBLIC_DASHBOARD_REFLEX_THRESHOLD=92
NEXT_PUBLIC_DASHBOARD_REFLEX_ENABLE_LOGGING=true
NEXT_PUBLIC_DASHBOARD_REFLEX_LOG_LEVEL=debug

# Local Development Settings
NEXT_PUBLIC_DASHBOARD_DEV_MODE=true
NEXT_PUBLIC_DASHBOARD_MOCK_DATA=true
NEXT_PUBLIC_DASHBOARD_HOT_RELOAD=true

# Dashboard Mock Data
NEXT_PUBLIC_DASHBOARD_MOCK_REFLEX_SCORE=88
NEXT_PUBLIC_DASHBOARD_MOCK_USER_COUNT=5
NEXT_PUBLIC_DASHBOARD_MOCK_SESSION_COUNT=12

# Dashboard Development Server
NEXT_PUBLIC_DASHBOARD_PORT=3000
NEXT_PUBLIC_DASHBOARD_HOST=localhost
